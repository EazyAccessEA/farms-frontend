{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000",
        "http://localhost:3000/map"
      ],
      "startServerCommand": "npm start",
      "startServerReadyPattern": "ready - started server on",
      "startServerReadyTimeout": 30000,
      "numberOfRuns": 3
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": 0.9}],
        "categories:accessibility": ["error", {"minScore": 0.95}],
        "categories:best-practices": ["error", {"minScore": 0.9}],
        "categories:seo": ["error", {"minScore": 0.9}],
        "first-contentful-paint": ["error", {"maxNumericValue": 1800}],
        "largest-contentful-paint": ["error", {"maxNumericValue": 1800}],
        "cumulative-layout-shift": ["error", {"maxNumericValue": 0.1}],
        "interaction-to-next-paint": ["error", {"maxNumericValue": 200}],
        "total-blocking-time": ["error", {"maxNumericValue": 200}],
        "speed-index": ["error", {"maxNumericValue": 2000}],
        "unused-css-rules": ["warn", {"maxLength": 0}],
        "unused-javascript": ["warn", {"maxLength": 0}],
        "modern-image-formats": ["error", {"minScore": 0.8}],
        "uses-optimized-images": ["error", {"minScore": 0.8}],
        "uses-text-compression": ["error", {"minScore": 0.8}],
        "uses-responsive-images": ["error", {"minScore": 0.8}],
        "efficient-animated-content": ["error", {"minScore": 0.8}],
        "preload-lcp-image": ["error", {"minScore": 0.8}],
        "total-byte-weight": ["error", {"maxNumericValue": 500000}],
        "max-potential-fid": ["error", {"maxNumericValue": 130}],
        "uses-rel-preload": ["error", {"minScore": 0.8}],
        "uses-rel-preconnect": ["error", {"minScore": 0.8}],
        "font-display": ["error", {"minScore": 0.8}],
        "resource-summary": ["error", {"maxNumericValue": 0}],
        "third-party-summary": ["error", {"maxNumericValue": 0}],
        "largest-contentful-paint-element": ["error", {"maxLength": 0}],
        "layout-shift-elements": ["error", {"maxLength": 0}],
        "long-tasks": ["error", {"maxLength": 0}],
        "non-composited-animations": ["error", {"maxLength": 0}],
        "unsized-images": ["error", {"maxLength": 0}],
        "valid-source-maps": ["error", {"minScore": 0.8}],
        "preload-fonts": ["error", {"minScore": 0.8}],
        "unminified-css": ["error", {"maxLength": 0}],
        "unminified-javascript": ["error", {"maxLength": 0}],
        "unused-css-rules": ["error", {"maxLength": 0}],
        "unused-javascript": ["error", {"maxLength": 0}],
        "uses-webp-images": ["error", {"minScore": 0.8}],
        "uses-avif-images": ["error", {"minScore": 0.8}],
        "image-size-responsive": ["error", {"minScore": 0.8}],
        "render-blocking-resources": ["error", {"maxLength": 0}],
        "unused-javascript": ["error", {"maxLength": 0}],
        "uses-long-cache-ttl": ["error", {"minScore": 0.8}],
        "dom-size": ["error", {"maxNumericValue": 1500}],
        "critical-request-chains": ["error", {"maxLength": 0}],
        "user-timings": ["error", {"maxLength": 0}],
        "bootup-time": ["error", {"maxNumericValue": 0}],
        "mainthread-work-breakdown": ["error", {"maxLength": 0}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
